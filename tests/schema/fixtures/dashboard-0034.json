{
    "dashboard": {
        "rooster-api-clone": {
            "rows": [
                {
                    "title": "Kong",
                    "showTitle": true,
                    "collapse": false,
                    "editable": true,
                    "height": "300px",
                    "panels": [
                        {
                            "datasource": {
                                "uid": "$datasource"
                            },
                            "description": "test timeseries",
                            "editable": true,
                            "error": false,
                            "fieldConfig": {
                                "defaults": {
                                    "color": {
                                        "mode": "continuous-GrYlRd"
                                    },
                                    "custom": {
                                        "axisLabel": "olala",
                                        "axisPlacement": "auto",
                                        "barAlignment": 0,
                                        "drawStyle": "line",
                                        "fillOpacity": 7,
                                        "gradientMode": "scheme",
                                        "hideFrom": {
                                            "legend": false,
                                            "tooltip": false,
                                            "viz": false
                                        },
                                        "lineInterpolation": "smooth",
                                        "lineStyle": {
                                            "dash": [
                                                5,
                                                10,
                                                15
                                            ],
                                            "fill": "dash"
                                        },
                                        "lineWidth": 2,
                                        "pointSize": 5,
                                        "scaleDistribution": {
                                            "type": "linear"
                                        },
                                        "showPoints": "never",
                                        "spanNulls": false,
                                        "stacking": {
                                            "group": "A",
                                            "mode": "none"
                                        },
                                        "thresholdsStyle": {
                                            "mode": "line+area"
                                        }
                                    },
                                    "decimals": 2,
                                    "displayName": "coba coba saja",
                                    "mappings": [
                                        {
                                            "options": {
                                                "7.62": {
                                                    "color": "red",
                                                    "index": 0,
                                                    "text": "asdfasdf"
                                                }
                                            },
                                            "type": "value"
                                        }
                                    ],
                                    "min": -2,
                                    "noValue": "-1.333",
                                    "thresholds": {
                                        "mode": "absolute",
                                        "steps": [
                                            {
                                                "color": "green",
                                                "value": null
                                            },
                                            {
                                                "color": "#F2495C",
                                                "value": 8
                                            }
                                        ]
                                    },
                                    "unit": "reqps"
                                },
                                "overrides": [
                                    {
                                        "matcher": {
                                            "id": "byName",
                                            "options": "coba coba saja"
                                        },
                                        "properties": [
                                            {
                                                "id": "color",
                                                "value": {
                                                    "fixedColor": "semi-dark-yellow",
                                                    "mode": "fixed"
                                                }
                                            }
                                        ]
                                    }
                                ]
                            },
                            "links": [
                                {
                                    "targetBlank": true,
                                    "title": "alink",
                                    "url": "https://deliveryhero.com"
                                }
                            ],
                            "options": {
                                "legend": {
                                    "calcs": [],
                                    "displayMode": "list",
                                    "placement": "bottom"
                                },
                                "tooltip": {
                                    "mode": "multi",
                                    "sort": "asc"
                                }
                            },
                            "targets": [
                                {
                                    "expr": "sum by (service) (irate(kong_http_status{service=~\"$service\"}[$__rate_interval]))",
                                    "legendFormat": "{{ service }}"
                                }
                            ],
                            "title": "Recent requests per second",
                            "type": "timeseries"
                        }
                    ]
                }
            ],
            "tags": [
                "grafyaml",
                "autogenerated",
                "default",
                "workforce"
            ],
            "templating": {
                "enabled": true,
                "list": [
                    {
                        "datasource": "blackhole",
                        "description": "Relevant if your service is behind Kong",
                        "includeAll": false,
                        "label": "Service",
                        "multi": true,
                        "name": "service",
                        "query": "label_values(kong_http_status{dh_env=\"$env\", service=~\".*$app.*\"}, service)",
                        "refresh": 1,
                        "type": "query"
                    }
                ]
            },
            "time": {
                "from": "now-15m",
                "to": "now"
            },
            "timezone": "browser",
            "title": "rooster-api-clone"
        }
    }
}